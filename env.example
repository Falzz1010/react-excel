# Supabase Configuration
# Get these values from your Supabase project dashboard
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Optional: Supabase Service Role Key (for server-side operations)
# VITE_SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Development Configuration
NODE_ENV=development

# Optional: Custom API endpoints
# VITE_API_BASE_URL=http://localhost:3000/api

# Optional: Feature flags
# VITE_ENABLE_DEBUG_MODE=true
# VITE_ENABLE_ANALYTICS=false

# Optional: File upload settings
# VITE_MAX_FILE_SIZE=10485760
# VITE_ALLOWED_FILE_TYPES=.xlsx,.xls,.csv

# Optional: Authentication settings
# VITE_AUTH_REDIRECT_URL=http://localhost:8080
# VITE_AUTH_PROVIDERS=email,google,github

# Optional: Database settings (if using custom database)
# VITE_DATABASE_URL=your_database_url_here

# Optional: Email service (if using custom email)
# VITE_EMAIL_SERVICE_URL=your_email_service_url
# VITE_EMAIL_API_KEY=your_email_api_key

# Optional: Storage settings
# VITE_STORAGE_BUCKET=your_storage_bucket_name
